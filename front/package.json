    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-router": "^4.0.31",
    "@types/react-router-dom": "^4.3.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-import-graphql": "^2.6.2",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "express": "^4.16.3",
