    "schema:codegen2": "apollo codegen:generate --addTypename --target=typescript --schema=src/graphql/schema.json --queries=src/graphql/*.ts --outputFlat=./typings/graphql.d.ts",
    "resolver": "ts-node graphql/deploy jw4s4q3gqbgkpbhr2pnk5uziji",
    "prod:build": "webpack --config webpack/webpack.prod.js"
