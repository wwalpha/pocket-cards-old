    "apollo": "^1.9.2",
    "apollo-boost": "^0.1.16",
    "apollo-client": "2.3.7",
    "aws-amplify": "^1.1.6",
    "aws-appsync": "^1.3.4",
    "aws-appsync-react": "^1.1.4",
    "classnames": "^2.2.6",
    "cpx": "^1.5.0",
    "graphql": "0.13.2",
    "graphql-tag": "^2.10.0",
    "react": "^16.5.2",
    "react-apollo": "2.1.9",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
